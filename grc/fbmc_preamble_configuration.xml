<?xml version="1.0"?>

<block>
    <name>CAZAC Preamble Configuration</name>
    <key>cazac_preamble_config</key>
    <category>FBMC</category>
    <import>import fbmc</import>
    <var_make>self.$(id) = $(id) = fbmc.preamble_config(taps=$taps, N=$N, L=$L, pos=$pos, u=$u, q=$q, A=$A)</var_make>
    <var_value>fbmc.preamble_config(taps=$taps, N=$N, L=$L, pos=$pos, u=$u, q=$q, A=$A)</var_value>
    <make></make>
    <param>
        <name>Filter Taps</name>
        <key>taps</key>
        <value></value>
        <type>float_vector</type>
    </param>
    <param>
        <name>Subcarriers</name>
        <key>N</key>
        <value>256</value>
        <type>int</type>
    </param>
    <param>
        <name>CAZAC periodic len</name>
        <key>L</key>
        <value>255</value>
        <type>int</type>
    </param>
    <param>
        <name>Block position</name>
        <key>pos</key>
        <value>3</value>
        <type>int</type>
    </param>
    <param>
        <name>u</name>
        <key>u</key>
        <value>1</value>
        <type>int</type>
    </param>
    <param>
        <name>q</name>
        <key>q</key>
        <value>4</value>
        <type>int</type>
    </param>
    <param>
        <name>Amplitude</name>
        <key>A</key>
        <value>1.0</value>
        <type>float</type>
    </param>
    <doc>
    Use Zadoff-Chu sequence to build an FBMC based preamble of length N
    https://en.wikipedia.org/wiki/Zadoff%E2%80%93Chu_sequence
    </doc>
</block>
