<block>
  <name>Frame sync</name>
  <key>fbmc_frame_sync_cvc</key>
  <category>FBMC</category>
  <import>import fbmc</import>
  <make>fbmc.frame_sync_cvc($L, $frame_len, $overlap, $preamble, $step_size, $threshold)</make>
  <param>
    <name>Num subcarriers</name>
    <key>L</key>
    <type>int</type>
  </param>
  <param>
    <name>Frame length</name>
    <key>frame_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Overlap</name>
    <key>overlap</key>
    <type>int</type>
  </param>
  <param>
    <name>Preamble</name>
    <key>preamble</key>
    <type>string</type>
  </param>
  <param>
    <name>Step size</name>
    <key>step_size</key>
    <type>int</type>
  </param>
  <param>
    <name>Threshold</name>
    <key>threshold</key>
    <type>float</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$L</vlen>
  </source>
</block>
