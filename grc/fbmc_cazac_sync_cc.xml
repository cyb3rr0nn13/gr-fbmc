<block>
  <name>Emergency Sync</name>
  <key>fbmc_cazac_sync_cc</key>
  <category>[FBMC]</category>
  <import>import fbmc</import>
  <make>fbmc.cazac_sync_cc($subcarriers, $bands, $overlap, $frame_len, $threshold, $zc_seqs, $zc_fft)</make>
  <param>
    <name>Subcarriers</name>
    <key>subcarriers</key>
    <type>int</type>
  </param>
  <param>
    <name>Bands</name>
    <key>bands</key>
    <type>int</type>
  </param>
  <param>
    <name>Overlap</name>
    <key>overlap</key>
    <type>int</type>
  </param>
  <param>
    <name>Correlation threshold</name>
    <key>threshold</key>
    <type>real</type>
  </param>
  <param>
    <name>Symbols per frame</name>
    <key>frame_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Mixed ZC sequences</name>
    <key>zc_seqs</key>
    <type>raw</type>
  </param>
  <param>
    <name>ZC FFT sequence</name>
    <key>zc_fft</key>
    <type>complex_vector</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
