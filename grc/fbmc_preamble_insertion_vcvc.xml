<block>
  <name>Preamble insertion</name>
  <key>fbmc_preamble_insertion_vcvc</key>
  <category>FBMC</category>
  <import>import fbmc</import>
  <make>fbmc.preamble_insertion_vcvc($L, $frame_len, $overlap, $channel_map, $prbs)</make>
  <param>
    <name>Num total carriers</name>
    <key>L</key>
    <type>int</type>
  </param>
  <param>
    <name>Sym per frame</name>
    <key>frame_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Overlap</name>
    <key>overlap</key>
    <type>int</type>
  </param>
  <param>
    <name>Channel map</name>
    <key>channel_map</key>
    <type>int_vector</type>
  </param>
  <param>
    <name>PRBS</name>
    <key>prbs</key>
    <type>complex_vector</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$L</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$L</vlen>
  </source>
</block>
