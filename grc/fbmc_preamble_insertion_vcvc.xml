<block>
  <name>Preamble insertion</name>
  <key>fbmc_preamble_insertion_vcvc</key>
  <category>FBMC</category>
  <import>import fbmc</import>
  <make>fbmc.preamble_insertion_vcvc($frame_len, $preamble_sym)</make>
  <param>
    <name>Frame_len</name>
    <key>frame_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Preamble_sym</name>
    <key>preamble_sym</key>
    <type>complex_vector</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>len($preamble_sym)</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>len($preamble_sym)</vlen>
  </source>
</block>
