<block>
  <name>Preamble insertion</name>
  <key>fbmc_preamble_insertion_vcvc</key>
  <category>FBMC</category>
  <import>import fbmc</import>
  <make>fbmc.preamble_insertion_vcvc($frame_len, $preamble)</make>
  <param>
    <name>Sym per frame</name>
    <key>frame_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Preamble</name>
    <key>preamble</key>
    <type>complex_vector</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$L</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$L</vlen>
  </source>
</block>
