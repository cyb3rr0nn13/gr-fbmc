<?xml version="1.0"?>

<block>
	<name>FBMC configuration</name>
	<key>variable_configuration</key>
	<category>FBMC</category>
	<import>import fbmc</import>
	<var_make>self.$(id) = $(id) = fbmc.fbmc_config(num_used_subcarriers=$M, num_payload_sym=$K, num_overlap_sym=$overlap, modulation=$mod, preamble=$preamble)</var_make>
	<make></make>
	<param>
		<name>Value</name>
		<key>value</key>
		<value>fbmc.fbmc_config(num_used_subcarriers=$M, num_payload_sym=$K, num_overlap_sym=$overlap, modulation=$mod, preamble=$preamble)</value>
		<type>raw</type>
	</param>
	<param>
		<name>Num used subcarriers</name>
		<key>M</key>
		<value>8</value>
		<type>int</type>
	</param>
	<param>
		<name>Num payload symbols</name>
		<key>K</key>
		<value>1</value>
		<type>int</type>
	</param>	<param>
		<name>Overlap</name>
		<key>overlap</key>
		<value>4</value>
		<type>int</type>
	</param>	<param>
		<name>Modulation type</name>
		<key>mod</key>
		<value>"QPSK"</value>
		<type>string</type>
	</param>	<param>
		<name>Preamble type</name>
		<key>preamble</key>
		<value>"IAM"</value>
		<type>string</type>
	</param>
</block>